<h1>Listing musics</h1>

<table>
  <tr>
    <th>File name</th>
    <th>File path</th>
    <th>Duration</th>
    <th>Date added</th>
    <th>Date modified</th>
    <th>File info</th>
    <th>Memo self</th>
    <th>Located at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @musics.each do |music| %>
  <tr>
    <td><%= music.file_name %></td>
    <td><%= music.file_path %></td>
    <td><%= music.duration %></td>
    <td><%= music.date_added %></td>
    <td><%= music.date_modified %></td>
    <td><%= music.file_info %></td>
    <td><%= music.memo_self %></td>
    <td><%= music.located_at %></td>
    <td><%= link_to 'Show', music %></td>
    <td><%= link_to 'Edit', edit_music_path(music) %></td>
    <td><%= link_to 'Destroy', music, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

